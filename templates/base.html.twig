<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Control de Asistencia{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
            <link href="{{ asset('/ControlAsistencia/public/css/materialIcons.css') }}" rel="stylesheet">
            <link href="{{ asset('/ControlAsistencia/public/css/bootstrap.css') }}" type="text/css" rel="stylesheet" />
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
            <script src="{{ asset('/ControlAsistencia/public/js/jquery-3.6.0.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('/ControlAsistencia/public/js/bootstrap.js') }}" type="text/javascript"></script>
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
